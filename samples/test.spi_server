# -*- coding: iso-8859-1 -*-

from __future__ import print_function, division
import	sys
if( sys.version_info[0] == 2 ):
	range = xrange

import	qm3.utils.spi

com = qm3.utils.spi.server( 10 )
com.barrier()
for i in range( 1, 10 ):
	tmp = com.recv_r8( i, 10 )
	print( i, tmp )
	com.send_r8( i, tmp )
com.barrier()
com.stop()
